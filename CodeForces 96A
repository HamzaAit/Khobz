word=input()
count=0

def func1():
	if count>=7:
		print("YES")
	else:
		print("NO")

length=""
def loop():
	for j in range(1, len(word)):
    	for i in range(j,len(word)):
    		global count
    		if word[i-1]==word[i]:
    			count+=1
    		else:
    			break
    	if count <=7:
    		break
    	
     	
    			
    		    
loop()
for i in range(1,len(word)):
	if word[i-1]!=word[i]:
    	global count
    	count=count
    	loop()
    	break

func1()        
            
    	
       
        
