word=input()
count=0

def func1():
	if count>=7:
		print("YES")
	else:
		print("NO")

length=""
def loop():
    for i in range(1,len(word)):
    	global count
    	if word[i-1]==word[i]:
    		count+=1
    		if count>=7:
    			print("Yes")
    			break
    		break
    	
    			
    		    
loop()
for i in range(1,len(word)):
	if word[i-1]!=word[i]:
    		global count
    		count=0
    		loop()
    		
    		break

        
            
    	
       
        
